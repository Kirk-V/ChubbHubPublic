<Application 
    x:Class="ChubbHubMVVM.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ChubbHubMVVM">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary 
                    Source="ResourceDictionaries/ClassHubDictionary.xaml"/>
                <ResourceDictionary
                    Source="ResourceDictionaries/MainViewDictionary.xaml"/>
                <ResourceDictionary
                    Source="ResourceDictionaries/DataGridDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
